class Parent {
   void method1() {
       System.out.println("Parent method1 is called");
       method2();
   }
 
   void method2() {
       System.out.println("Parent method2 is called");
   }
}
 
class Child extends Parent {
   @Override
   void method1() {
       System.out.println("Child method1 is called");
       super.method1();
   }
 
   @Override
   void method2() {
       System.out.println("Child method2 is called");
       super.method2();
   } 
   public static void main(String[] args) {
       // Parent reference to Parent object
       Parent p1 = new Parent();
       p1.method1();
//OUTPUT: Parent method1 is called
//Parent method2 is called
 
       System.out.println("-");
 
       // Child reference to Child object
       Child c1 = new Child();
       c1.method1();

//OUTPUT:
// Child method1 is called
 //Parent method1 is called
//Child method2 is called
//Parent method2 is called


       System.out.println("-");
 
       // Parent reference to Child object 
       Parent p2 = new Child();
       p2.method1();

///OUTPUT: Child method1 is called
 //Parent method1 is called
//Child method2 is called
//Parent method2 is called

       System.out.println("");
   }
}
